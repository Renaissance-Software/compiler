GetStdHandle :: (std_handle: s32) u64 extern;
ExitProcess :: (exit_code: u32) extern;
WriteFile :: (handle: u64, buffer: u64, bytes_to_be_written: u32, bytes_written: *u32, overlapped: u64) u32 extern;

main :: ()
{
    stdout_handle_index: s32 = -11;
    hello_world_str : []u8 = "Hello world\n";
    stdout_handle: u64 = GetStdHandle(stdout_handle);
    written: u32 = 0;
    WriteFile(stdout_handle, hello_world_str.ptr, hello_world_str.len, &written, 0);
}
